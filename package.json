{
  "name": "heliosrx-example-fitness-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "webpack --env.prod",
    "rules": "npm run rules:make && npm run rules:compile && npm run rules:deploy",
    "rules:compile": "firebase-bolt database.rules.bolt",
    "rules:deploy": "firebase deploy --only database",
    "rules:make": "helios rules --write database.rules.bolt"
  },
  "dependencies": {
    "core-js": "^3.6.3",
    "dayjs": "^1.8.19",
    "firebase": "^7.6.2",
    "heliosrx": "file:../heliosrx",
    "heliosrx-cli": "file:../heliosrx/packages/cli",
    "milligram": "^1.3.0",
    "vue": "^3.0.0-alpha.4",
    "vue-ionicons": "^3.0.4",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.2",
    "@vue/compiler-sfc": "^3.0.0-alpha.1",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "url-loader": "^3.0.0",
    "vue-loader": "^16.0.0-alpha.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "npm-run-all": "^4.1.5"
  }
}
