{
  "name": "heliosrx-example-fitness-tracker2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "rules": "npm run rules:make && npm run rules:compile && npm run rules:deploy",
    "rules:compile": "firebase-bolt database.rules.bolt",
    "rules:deploy": "firebase deploy --only database",
    "rules:make": "helios rules --write database.rules.bolt"
  },
  "dependencies": {
    "core-js": "^3.6.3",
    "firebase": "^7.6.2",
    "heliosrx": "https://github.com/tw00/heliosrx-private.git",
    "milligram": "^1.3.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.1",
    "vue": "^2.6.11",
    "vue-ionicons": "^3.0.4",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "vue-template-compiler": "^2.6.11"
  }
}
